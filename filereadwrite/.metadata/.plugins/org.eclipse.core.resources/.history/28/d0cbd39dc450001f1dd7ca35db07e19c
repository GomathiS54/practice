package com.example.files;

import java.io.UnsupportedEncodingException;
import java.net.URLEncoder;

import jakarta.persistence.criteria.Path;

public class FilePath {


	    public static void main(String[] args) {
	        try {
	            String filePath = "C:/jiraapi/filereadwrite";
	            String encodedFilePath = URLEncoder.encode(filePath, "UTF-8");
	            System.out.println("Encoded FilePath: " + encodedFilePath);
	        } catch (UnsupportedEncodingException e) {
	            e.printStackTrace();
	        }
	    }
	    
	    
	    public static Path createPath(String pathStr) {
	        return PathStr.get(pathStr);
	    }
	}


